    "version": "0.2.0",
    "configurations": [
        {
            "name": "espresso_log mock scale",
            "request": "launch",
            "type": "dart",
            "args":[
                "--dart-define", "USE_MOCK_SCALE=true",
			]
        },
        {
            "name": "espresso_log mock pressure",
            "request": "launch",
            "type": "dart",
            "args":[
                "--dart-define", "USE_MOCK_PRESSURE=true",
			]
        },
        {
            "name": "espresso_log mock all",
            "request": "launch",
            "type": "dart",
            "args":[
                "--dart-define", "USE_MOCK_PRESSURE=true", "USE_MOCK_SCALE=true",
			]
        },
        {
            "name": "espresso_log",
            "request": "launch",
            "type": "dart",
        },
        {
            "name": "espresso_log (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "espresso_log (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}